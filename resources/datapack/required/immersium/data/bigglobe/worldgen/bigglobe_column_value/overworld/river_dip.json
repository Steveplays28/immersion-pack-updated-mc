{
	"type": "script",
	"params": {
		"type": "double",
		"is_3d": false
	},
	"script": [
		"double dip = `bigglobe:overworld/river_depth` / (`bigglobe:overworld/raw_rivers` ^ 4 + 1.0L)",
		"dip *= smooth((`bigglobe:overworld/sea_level` - 24.0L - `bigglobe:overworld/raw_erosion`) / `bigglobe:overworld/river_depth`)",
		"dip *= unmixSmooth(0.12L, 0.0L, `bigglobe:overworld/hilliness` * `bigglobe:overworld/continentalness`)",
		"return(dip)"
	]
}
